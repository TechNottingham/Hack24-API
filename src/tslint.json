{
  "extends": "tslint:recommended",
  "rules": {
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "no-conditional-assignment": false,
    "no-var-requires": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-pascal-case",
      "allow-leading-underscore",
      "allow-trailing-underscore"
    ],
    "object-literal-sort-keys": false,
    "switch-default": false,
    "max-line-length": [
      true,
      160
    ],
    "no-string-literal": false,
    "interface-name": [
      true,
      "never-prefix"
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-shorthand": false,
    "no-reference": true,
    "import-spacing": true,
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-module",
      "check-separator",
      "check-type",
      "check-typecast"
    ]
  },
  "rulesDirectory": []
}
